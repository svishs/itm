  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: текущий файл",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },

    {
      "name": "Sync ORM",
      "type": "python",
      "request": "launch",
      "program": "src/main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["--sync", "--orm"]
    },
    {
      "name": "Sync Core",
      "type": "python",
      "request": "launch",
      "program": "src/main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["--sync", "--core"]
    },
    {
      "name": "Async ORM",
      "type": "python",
      "request": "launch",
      "program": "src/main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["--async", "--orm"]
    },
    {
      "name": "Async Core",
      "type": "python",
      "request": "launch",
      "program": "src/main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["--async", "--core"]
    }
  ]
}
